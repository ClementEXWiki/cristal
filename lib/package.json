{
  "name": "@cristal/lib",
  "version": "0.7.0",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "directory": "lib",
    "url": "https://github.com/xwiki-contrib/cristal/"
  },
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "main": "./src/index.ts",
  "scripts": {
    "build": "pnpm compile",
    "clean": "rimraf dist",
    "compile": "vue-tsc --project tsconfig.json --noEmit && vite build",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings=0",
    "prepack": "pnpm build",
    "test": "echo 'no test'"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@cristal/api": "workspace:*",
    "@cristal/browser-api": "workspace:*",
    "@cristal/dsfr": "workspace:*",
    "@cristal/dsshoelace": "workspace:*",
    "@cristal/dsvuetify": "workspace:*",
    "@cristal/editors-tiptap": "workspace:*",
    "@cristal/extension-manager": "workspace:*",
    "@cristal/extension-menubuttons": "workspace:*",
    "@cristal/macros": "workspace:*",
    "@cristal/rendering": "workspace:*",
    "@cristal/sharedworker-impl": "workspace:*",
    "@cristal/skin": "workspace:*",
    "@cristal/storage": "workspace:*",
    "@mdi/font": "7.4.47",
    "inversify": "6.0.2",
    "reflect-metadata": "0.2.2",
    "utility-types": "3.11.0",
    "vue": "3.4.27",
    "vue-i18n": "9.13.1",
    "vue-router": "4.3.2",
    "vuetify": "3.6.7"
  },
  "devDependencies": {
    "rimraf": "5.0.7",
    "vite": "5.2.11",
    "vue-tsc": "2.0.19"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.es.js",
        "require": "./dist/index.umd.js",
        "types": "./dist/index.d.ts"
      }
    },
    "main": "./dist/index.es.js",
    "types": "./dist/index.d.ts"
  }
}
