{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "lint"
        ]
      }
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*"
    ]
  },
  "targetDefaults": {
    "build": {
      "inputs": [
        "default",
        "^default"
      ],
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "compile": {
      "inputs": [
        "default",
        "^default"
      ],
      "dependsOn": [
        "^compile"
      ]
    }
  }
}
